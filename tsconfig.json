{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./assets/js",
    "jsx": "react",
    "lib": ["es2017", "es2015.promise", "dom"],
    "module": "es6",
    "moduleResolution": "node",
    "keyofStringsOnly": false,
    "strictNullChecks": true,
    "sourceMap": true,
    "target": "es5"
  },
  "exclude": ["node_modules", "deploy", "dist"]
}
